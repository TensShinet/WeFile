{
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "schemes": [
    "http",
    "https"
  ],
  "swagger": "2.0",
  "info": {
    "description": "WeFile API",
    "version": "0.0.1"
  },
  "basePath": "/api/v1",
  "paths": {
    "/role/all": {
      "get": {
        "description": "获得所有角色",
        "tags": [
          "Role"
        ],
        "summary": "GetAllRoles",
        "operationId": "GetAllRoles",
        "responses": {
          "200": {
            "$ref": "#/responses/RoleList"
          },
          "500": {
            "$ref": "#/responses/ErrorResponse"
          }
        }
      }
    },
    "/user/sign_in": {
      "post": {
        "description": "用户登录",
        "tags": [
          "User"
        ],
        "summary": "SignIn",
        "operationId": "SignIn",
        "parameters": [
          {
            "x-go-name": "Body",
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "required": [
                "email",
                "password"
              ],
              "properties": {
                "email": {
                  "description": "邮箱",
                  "type": "string",
                  "x-go-name": "Email"
                },
                "password": {
                  "description": "密码",
                  "type": "string",
                  "x-go-name": "Password"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/responses/UserResponse"
          },
          "403": {
            "$ref": "#/responses/ForbiddenResponse"
          },
          "500": {
            "$ref": "#/responses/ErrorResponse"
          }
        }
      }
    },
    "/user/sign_up": {
      "post": {
        "description": "用户注册",
        "tags": [
          "User"
        ],
        "summary": "SignUp",
        "operationId": "SignUp",
        "parameters": [
          {
            "x-go-name": "User",
            "description": "新用户",
            "name": "user",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/NewUser"
            }
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/responses/UserResponse"
          },
          "409": {
            "$ref": "#/responses/ConflictError"
          },
          "500": {
            "$ref": "#/responses/ErrorResponse"
          }
        }
      }
    }
  },
  "definitions": {
    "NewUser": {
      "type": "object",
      "required": [
        "name",
        "password",
        "email",
        "verify_code",
        "RoleID"
      ],
      "properties": {
        "RoleID": {
          "description": "角色 ID",
          "type": "integer",
          "format": "int64"
        },
        "email": {
          "description": "邮箱\n\n邮箱唯一",
          "type": "string",
          "x-go-name": "Email"
        },
        "name": {
          "description": "用户名",
          "type": "string",
          "maxLength": 64,
          "minLength": 10,
          "x-go-name": "Name"
        },
        "password": {
          "description": "密码",
          "type": "string",
          "maxLength": 64,
          "minLength": 10,
          "x-go-name": "Password"
        },
        "profile": {
          "description": "个人介绍",
          "type": "string",
          "x-go-name": "Profile"
        },
        "verify_code": {
          "description": "验证码\n\n邮件拿验证码",
          "type": "string",
          "x-go-name": "VerifyCode"
        }
      },
      "x-go-package": "github.com/TensShinet/WeFile/service/base/handler"
    },
    "Role": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "x-go-name": "Name"
        },
        "role_id": {
          "type": "integer",
          "format": "int64",
          "x-go-name": "RoleID"
        }
      },
      "x-go-package": "github.com/TensShinet/WeFile/service/base/handler"
    },
    "User": {
      "type": "object",
      "properties": {
        "email": {
          "description": "邮箱",
          "type": "string",
          "x-go-name": "Email"
        },
        "name": {
          "description": "用户名",
          "type": "string",
          "x-go-name": "Name"
        },
        "profile": {
          "description": "个人简介",
          "type": "string",
          "x-go-name": "Profile"
        },
        "role_id": {
          "description": "角色 id",
          "type": "integer",
          "format": "int64",
          "x-go-name": "RoleID"
        },
        "role_name": {
          "description": "角色名",
          "type": "string",
          "x-go-name": "RoleName"
        },
        "user_id": {
          "description": "用户 id",
          "type": "integer",
          "format": "int64",
          "x-go-name": "UserID"
        }
      },
      "x-go-package": "github.com/TensShinet/WeFile/service/base/handler"
    }
  },
  "responses": {
    "ConflictError": {
      "description": "Conflict Error",
      "headers": {
        "message": {
          "type": "string"
        }
      }
    },
    "ErrorResponse": {
      "description": "Error Message",
      "headers": {
        "message": {
          "type": "string"
        }
      }
    },
    "ForbiddenResponse": {
      "description": "Forbidden Error",
      "headers": {
        "message": {
          "type": "string"
        }
      }
    },
    "NotFoundError": {
      "description": "NotFound Error",
      "headers": {
        "message": {
          "type": "string"
        }
      }
    },
    "RoleList": {
      "description": "Role List",
      "schema": {
        "type": "array",
        "items": {
          "$ref": "#/definitions/Role"
        }
      }
    },
    "UserResponse": {
      "schema": {
        "$ref": "#/definitions/User"
      },
      "headers": {
        "cookie": {
          "type": "string",
          "description": "in: header"
        }
      }
    }
  }
}